{
  "name": "softia_data_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run swagger",
    "dev": "concurrently \"next dev\"  \"nodemon tsoa spec\"",
    "prebuilddevelop": "npm run swagger",
    "build": "next build",
    "prestart": "npm run build",
    "start": "node src/api/server.ts",
    "lint": "next lint",
    "test": "jest",
    "swagger": "tsoa spec-and-routes"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [
      "routes.js"
    ],
    "ext": "ts",
    "exec": "ts-node src/api/server.ts"
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "typings",
      "utils"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "d.ts"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^2.6.2",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@tanstack/react-query": "^4.29.12",
    "@types/node": "20.3.0",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "morgan": "^1.10.0",
    "next": "^13.4.5",
    "next-drupal": "^1.6.0",
    "next-seo": "^6.0.0",
    "nodemon": "^2.0.22",
    "nprogress": "^0.2.0",
    "pg": "^8.11.1",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sinon": "^15.2.0",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^4.6.3",
    "tailwindcss": "3.3.2",
    "tsoa": "^5.1.1"
  },
  "description": "Softia data App and Api",
  "main": "drupal.config.js",
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/morgan": "^1.9.4",
    "@types/swagger-ui-express": "^4.1.3",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.2.0",
    "jest": "^29.6.1",
    "sequelize-cli": "^6.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
